	pages = {505--547},
	file = {Full Text PDF:/Users/michellehoda/Zotero/storage/94QVXEKV/Wisittanawat and Lehrer - 2024 - Teacher Cultivation of Classroom Statistical Modeling Practice A Case Study.pdf:application/pdf},
}

