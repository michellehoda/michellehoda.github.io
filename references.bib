@article{wart_scripts_2019,
	title = {Scripts and {Counterscripts} in {Community}-{Based} {Data} {Science}: {Participatory} {Digital} {Mapping} and the {Pursuit} of a {Third} {Space}},
	volume = {29},
	shorttitle = {Scripts and {Counterscripts} in {Community}-{Based} {Data} {Science}},
	doi = {10.1080/10508406.2019.1693378},
	abstract = {Data increasingly mediates how we understand the world. As such, there is growing interest in designing initiatives to help young people learn about data—not only the techno-mathematical skills necessary to work with data, but also the dispositions needed to participate in data-centric ways of knowing and doing. In this article, we argue that as this educational goal is pursued, it is important to attend to the normative scripts that are often associated with data, and how they relate to learners’ perspectives and prior experiences. We do this by examining two initiatives that aimed to help young people learn about data and its “real-world” applications by engaging them in participatory mapping activities, directed toward the study of local community challenges. We argue that when there are mismatches between students’ realities and how reality is described to work in data science, making the time and space to examine these contradictions can lead to a robust engagement with data science and its applications. These findings have implications for how we might better design tools and learning activities that connect data science with the broader contexts that frame young people’s lives.},
	journal = {Journal of the Learning Sciences},
	author = {Wart, Sarah and Lanouette, Kathryn and Parikh, Tapan},
